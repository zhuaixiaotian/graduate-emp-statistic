<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.roadjava.statistic.mapper.StudentJobMapper">
  <insert id="insert">
    insert into student_job (
      student_id,
      job_id
    ) values (
      #{studentId},
      #{jobId}
    )
  </insert>
  <delete id="deleteById">
    delete from student_job where id = #{id}
  </delete>

</mapper>